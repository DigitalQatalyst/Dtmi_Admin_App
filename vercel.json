{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "functions": {
    "api/uploads/put.ts": {
      "maxDuration": 60
    },
    "api/uploads/sign.ts": {
      "maxDuration": 10
    }
  }
}

